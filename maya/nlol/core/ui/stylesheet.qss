QPushButton {
    background-color: rgb(50, 50, 50);
    color: rgb(210, 210, 210);
    border-radius: 8px;
    padding: 3px 3px;
    font-size: 12px;
    text-align: left;
    padding-left: 10px;
}

QPushButton:hover {
    background-color: rgb(85, 85, 85);
}

QPushButton:pressed {
    background-color: rgb(90, 90, 90);
    border: 1px solid rgb(90, 90, 90);
}